legacy-peer-deps=true
strict-peer-deps=false
# Build optimizations
engine-strict=true
audit=false
fund=false
# Cache optimizations
prefer-offline=true
# Reduce network requests
progress=false
loglevel=error
# Performance
maxsockets=50
fetch-retries=2
fetch-retry-mintimeout=10000
fetch-retry-maxtimeout=60000
